<h2><%=l(:label_version)%></h2>

<% labelled_tabular_form_for :version, @version, :url => { :action => 'edit' } do |f| %>
<%= render :partial => 'versions/form', :locals => { :f => f } %>
<%= submit_tag l(:button_save) %> 
<%= link_to "Cancel", url_for(:back), :class => 'icon icon-cancel' %>
<% end %>
